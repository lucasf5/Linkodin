    "/register": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "description": "Register",
        "summary": "Summary: Register",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "nome_usuario": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "senha": {
                    "type": "string"
                  },
                  "usuario_tipo": {
                    "type": "string"
                  },
                  "cpf_cnpj": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Ok"
          },
          "400": {
            "description": "Could not register this user"
          }
        }
      }
    },

    "/login": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Summary: Login",
        "description": "Login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "senha": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Ok"
          },
          "400": {
            "description": "Unable to login this user"
          }
        }
      }
    },
    
